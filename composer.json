{
    "name": "aplr/laravel-passport-grant-anonymous",
    "description": "Anonymous Grant for Laravel Passport.",
    "keywords": ["laravel", "passport", "oauth"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/aplr/laravel-passport-grant-anonymous/issues",
        "source": "https://github.com/aplr/laravel-passport-grant-anonymous"
    },
    "authors": [
        {
            "name": "Andreas Pfurtscheller",
            "email": "hello@aplr.me"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "5.3.x|5.4.x|5.5.x|5.6.x",
        "laravel/passport": "^3.0|^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Aplr\\LaravelPassportAnonymous\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aplr\\LaravelPassportAnonymous\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}